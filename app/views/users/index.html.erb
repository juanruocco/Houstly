<h1>Users!</h1>
<table>
  <tr>
    <th>Email</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Birthdate</th>
    <th>Sex</th>
    <th>Type License</th>
    <th>Expiration Date</th>
  </tr>
  <% @users.each do |userit| %>
      <%= form_for userit do |f| %>
      <tr>
        <td><%= f.text_field :email %></td>
        <td><%= f.text_field :first_name %></td>
        <td><%= f.text_field :last_name %></td>
        <td><%= f.text_field :birthdate %></td>
        <td><%= f.text_field :sex %></td>
        <td><%= f.text_field :type_license %></td>
        <td><%= f.text_field :expiration_date %></td>
        <td>
          <%= f.submit 'Update', class: 'btn btn-primary' %>
        </td>
        <td>
            <%= f.submit 'Delete', class: 'btn btn-primary' %>
         </td>
      </tr>
      <% end %>
  <% end %>
</table>
